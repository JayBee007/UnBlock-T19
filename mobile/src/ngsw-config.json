{
  "index": "/index.html",
  "name": "Hack Core Blockchain Wallet", 

  "short_name": "Hack Core", 

  "start_url": "index.html", 

  "display": "standalone", 

  "icons": [{ 
    "src": "assets/images/logo-pwa.png", 
    "sizes": "192x192", 
    "type": "image/png" 
  }],
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/",
        "/favicon.ico",
        "/index.html",
        "/wallet",
        "/blocks",
        "/history"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }, {
    "name": "fonts",
    "resources": {
      "urls": [
        "https://fonts.googleapis.com/**",
        "https://fonts.gstatic.com/**"
      ]
    }
  }]
}